package day.pkg16.wana;
import java.util.Scanner;

public class Day16Wana {

    public static void main(String[] args) {
        Scanner input = new Scanner (System.in);
        System.out.print("Masukkan Nilai : ");
        int nilai = input.nextInt();
        if (nilai >= 70 && nilai <=80) {
            System.out.println(nilai + " adalah nilai B");
        }
        else if (nilai <= 70) {
            System.out.println(nilai + " adalah nilai C");
        }
        else if (nilai >= 85) {
            System.out.println(nilai + " adalah nilai A");
        }
        else {
            System.out.println(" Tidak Memiliki Nilai");
        }

    }
    
}
